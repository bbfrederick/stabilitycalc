set terminal jpeg
 set autoscale
 unset log
 unset label
 set xdata time
 set xtics autofreq rotate
 set ytic auto
 set timefmt "%Y%m%dT%H:%M:%S"
set xrange ["20091120T00:00:00":]
set yrange [0.9:1.1]
set title "Phantom shape (y/x)"
set xlabel "Date"
set ylabel "Phantom shape (y/x)"
plot    "${outputdirectory}/${whichscan}/graphtemp" using 2:16 title 'Object shape (y/x)' with ${wlp} ps 2 pt 3

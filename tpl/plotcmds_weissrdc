set terminal jpeg
 set autoscale
 unset log
 unset label
 set xdata time
 set xtics autofreq rotate
 set ytic auto
 set timefmt "%Y%m%dT%H:%M:%S"
set xrange ["20091120T00:00:00":]
set yrange [0:12.0]
set title "Weisskoff Radius of decorrelation"
set xlabel "Date"
set ylabel "Weisskoff RDC"
plot    "${outputdirectory}/${whichscan}/graphtemp" using 2:23 title 'Weisskoff RDC' with ${wlp} ps 2 pt 3

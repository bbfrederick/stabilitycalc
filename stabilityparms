#!/usr/bin/env python
#

import sys

if len(sys.argv) != 2:
    print("usage: stabilityparms key")
    exit()
thekey = sys.argv[1]

confdict = {}
theconffile = open("configuration.txt")
for line in theconffile:
    thepair = line.split()
    confdict[thepair[0]] = thepair[1]
theconffile.close()

print(confdict.get(thekey,''))
